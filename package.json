{
  "publisher": "czhlin",
  "name": "ext-name",
  "displayName": "ext-name",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/ext-name#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/ext-name"
  },
  "bugs": {
    "url": "https://github.com/antfu/ext-name/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "node": ">=18",
    "vscode": "^1.97.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World: Show"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "ext-name",
      "properties": {}
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output ext/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies && ovsx publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "pack:pre": "vsce package --no-dependencies --pre-release",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release:pre": "vsce publish --no-dependencies --pre-release",
    "release": "bumpp && nr publish"
  },
  "dependencies": {
    "@tomjs/vscode-webview": "^2.0.0",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "query-string": "^9.2.2",
    "vue": "^3.5.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@antfu/ni": "^23.3.1",
    "@tomjs/tsconfig": "^1.7.2",
    "@tomjs/vite-plugin-vscode": "v4.2.1",
    "@tomjs/vscode-webview": "^2.0.0",
    "@types/node": "^22.13.4",
    "@types/vscode": "^1.97.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vscode/vsce": "^3.2.2",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "esno": "^4.8.0",
    "ovsx": "^0.10.5",
    "pnpm": "^10.4.1",
    "reactive-vscode": "^0.2.10",
    "typescript": "^5.7.3",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.0.5",
    "vscode-ext-gen": "^1.0.0",
    "vue-tsc": "^2.2.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
